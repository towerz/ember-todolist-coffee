{
  "name": "todolist",
  "version": "0.0.0",
  "description": "Ember sample todo list app",
  "main": "index.js",
  "scripts": {
    "test": "test"
  },
  "author": "Bruno Torres",
  "license": "BSD",
  "devDependencies": {
    "browserify": "^4.1.11",
    "browserify-shim": "^3.5.0",
    "glob": "^4.0.2",
    "gulp": "^3.8.1",
    "gulp-browserify": "^0.5.0",
    "gulp-changed": "^0.4.0",
    "gulp-coffee": "^2.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^0.7.2",
    "gulp-util": "^2.2.17",
    "mkdirp": "^0.5.0",
    "path": "^0.4.9",
    "shimbro": "0.0.11",
    "underscore": "^1.6.0",
    "wiredep": "^1.8.1"
  },
  "dependencies": {},
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "handlebars": "Handlebars",
    "ember": {
      "depends": [
        "jquery:jQuery",
        "handlebars:Handlebars"
      ],
      "exports": "Ember"
    },
    "ember-data": {
      "depends": [
        "ember:Ember"
      ],
      "exports": "DS"
    },
    "ember-localstorage-adapter": {
      "depends": [
        "ember-data:DS",
        "ember:Ember"
      ],
      "exports": "DS.LSAdapter"
    }
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "handlebars": "./bower_components/handlebars/handlebars.js",
    "ember": "./bower_components/ember/ember.js",
    "ember-data": "./bower_components/ember-data/ember-data.js",
    "ember-localstorage-adapter": "./bower_components/ember-localstorage-adapter/localstorage_adapter.js"
  }
}
